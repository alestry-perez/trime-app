{
  "expo": {
    "name": "Trime-App-Demo",
    "slug": "trimeapp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/pics/icon.png",
    "splash": {
      "image": "./app/assets/whole-splashscreen@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apz.TrimeApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/pics/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.apz.TrimeApp"
    },
    "web": {
      "favicon": "./app/assets/pics/favicon.png"
    },
    "description": "Trime App\nA HyperIsland Student Project\nDesign & Frontend Development Project\n\nhttp://trime.app/\n\nDeveloped by:\nElla Söder\nGuðmundur Björnsson\nLovisa Elensky\nSupipi Algama\nAlestry F. Pérez",
    "githubUrl": "https://github.com/alestry-perez/trime-app"
  }
}
