{
  "expo": {
    "name": "TrimeApp",
    "slug": "TrimeApp",
    "version": "1.0.0",
    "orientation": "portrait",
    "icon": "./app/assets/pics/icon.png",
    "splash": {
      "image": "./app/assets/whole-splashscreen@3x.png",
      "resizeMode": "contain",
      "backgroundColor": "#ffffff"
    },
    "updates": {
      "fallbackToCacheTimeout": 0
    },
    "assetBundlePatterns": [
      "**/*"
    ],
    "ios": {
      "supportsTablet": true,
      "bundleIdentifier": "com.apz.TrimeApp"
    },
    "android": {
      "adaptiveIcon": {
        "foregroundImage": "./app/assets/pics/adaptive-icon.png",
        "backgroundColor": "#FFFFFF"
      },
      "package": "com.apz.TrimeApp"
    },
    "web": {
      "favicon": "./app/assets/pics/favicon.png"
    },
    "description": ""
  }
}
